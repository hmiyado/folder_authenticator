# Default environment variables for Fastlane
# These are fallback values used when variables are not set elsewhere

# Build configuration
FASTLANE_SKIP_WAITING_FOR_BUILD_PROCESSING=true
FASTLANE_HIDE_GITHUB_ISSUES=true

# Default timeout values (in seconds)
FASTLANE_XCODEBUILD_SETTINGS_TIMEOUT=120
FASTLANE_XCODE_LIST_TIMEOUT=10

# Disable automatic updates
FASTLANE_SKIP_UPDATE_CHECK=true